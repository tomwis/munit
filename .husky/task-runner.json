{
   "tasks": [
      {
         "name": "commit-message-linter",
         "group": "commit-msg",
         "command": "dotnet",
         "args": [
            "husky",
            "exec",
            ".husky/csx/commit-lint.csx",
            "--args",
            "${args}",
            "conventionalcommit.json"
         ]
      }
   ]
}
